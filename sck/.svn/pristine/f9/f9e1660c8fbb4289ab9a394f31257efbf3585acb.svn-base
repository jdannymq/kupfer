package org.domain.sck.dto;

public enum EstadoNivelServicioSolicitud {
	NO_APLICA, CUMPLIDA, NO_CUMPLIDA;
	
	public boolean isCumplida() {
		if(this.equals(CUMPLIDA)) {
			return true;
		}
		return false;
	}
	
	public boolean isNoAplica() {
		if(this.equals(NO_APLICA)) {
			return true;
		}
		return false;
	}
	
	public boolean isNoCumplida() {
		if(this.equals(NO_CUMPLIDA)) {
			return true;
		}
		return false;
	}
}
