package org.domain.sck.dto;

import java.util.List;

import org.domain.sck.entity.ConceptosNegocio;
import org.domain.sck.entity.DetalleCp;

public class DetalleCotPedDTO {
	
	public DetalleCotPedDTO(){
		
	}
	
	private ConceptosNegocio negocio;
	private List<DetalleCp> listaDetalleCp;
	private Long montoNeto;
	private Long monto;
	private Double PorceDelTotal;
	private Double porcePorConcepto;
	private Double margenConcepto;

	public List<DetalleCp> getListaDetalleCp() {
		return listaDetalleCp;
	}
	public void setListaDetalleCp(List<DetalleCp> listaDetalleCp) {
		this.listaDetalleCp = listaDetalleCp;
	}
	public ConceptosNegocio getNegocio() {
		return negocio;
	}
	public void setNegocio(ConceptosNegocio negocio) {
		this.negocio = negocio;
	}
	public void setMonto(Long monto) {
		this.monto = monto;
	}
	public Long getMonto() {
		return monto;
	}
	public Double getPorceDelTotal() {
		return PorceDelTotal;
	}
	public void setPorceDelTotal(Double porceDelTotal) {
		PorceDelTotal = porceDelTotal;
	}
	public Double getPorcePorConcepto() {
		return porcePorConcepto;
	}
	public void setPorcePorConcepto(Double porcePorConcepto) {
		this.porcePorConcepto = porcePorConcepto;
	}
	public Long getMontoNeto() {
		return montoNeto;
	}
	public void setMontoNeto(Long montoNeto) {
		this.montoNeto = montoNeto;
	}
	public Double getMargenConcepto() {
		return margenConcepto;
	}
	public void setMargenConcepto(Double margenConcepto) {
		this.margenConcepto = margenConcepto;
	}
	
}
