	public LcredCargos sacarCargoUsuario(String codigo){
		LcredCargos cargo = null;
		try{
			if(listaCargos != null){
				for(LcredCargos lc : listaCargos){
					if(lc.getCodCargo().equals(codigo)){
						cargo = lc;
						break;
					}
				}
			}
		}catch (Exception e) {
			log.error("Error, al buscar el cargo.", e.getMessage());
		}
		return cargo;
	}
