    public List<GraficoDTO> getGraficoCumplidas() {
    	List<GraficoDTO> graficoData =  new ArrayList<GraficoDTO>(2);
    	GraficoDTO grafico = new GraficoDTO();
    	grafico.setLabel("Cumplidas");
    	grafico.setCantidad(this.totalCumplidas.size());
    	graficoData.add(grafico);
    	grafico = new GraficoDTO();
    	grafico.setLabel("No Cumplidas");
    	grafico.setCantidad(this.totalNoCumplidas.size());
    	graficoData.add(grafico);
